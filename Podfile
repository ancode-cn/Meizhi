# install/update请执行命令：pod install --verbose --no-repo-update

# SnapKit 代码编写AutoLayout
source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '8.0'
use_frameworks!

pod 'SnapKit', '~> 0.12.0'

# Alamofire网络库
source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '8.0'
use_frameworks!

pod 'Alamofire', '~> 1.3.1'

# json解析
platform :ios, '8.0'
use_frameworks!

pod 'SwiftyJSON', '~> 2.2.1'

# SDWebImage图片缓存库
platform :ios, '6.1'
pod 'SDWebImage', '~>3.7'

# PagingMenuController导航
source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '8.0'
use_frameworks!

pod "PagingMenuController"

# pod 'PageMenu'
pod 'PageMenu'

# MJRefresh 下拉刷新
pod 'MJRefresh'